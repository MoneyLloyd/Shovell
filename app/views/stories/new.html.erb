<h1>New story</h1>

    <%= error_messages_for :story %>
  
<% form_for(@story) do |f| %>
  <p>
    <b>Name</b><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <b>Link</b><br />
    <%= f.text_field :link %>
  </p>

  <p>description:<br />
    <%= f.text_area :description %>
  </p>

  <p>
    tags:<br />
    <%= f.text_field :tag_list %>
  </p>
    
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', stories_path %>
